<!DOCTYPE html>
<html data-ng-app="Messanger">
<head>
    <meta content="IE=edge, chrome=1" http-equiv="X-UA-Compatible" />
    <title>ChatApp</title>
    <script src="/Scripts/jquery-1.8.2.min.js"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <link href="//netdna.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
    
    <link rel="stylesheet" href="/Content/css/main.css">
    <link href="~/Content/css/angular-busy.css" rel="stylesheet"/>
    <script type="text/ng-template" id="Background.html">
        <div style="background-image:url('../../Content/loading.gif');background-position:50% 20px;background-repeat:no-repeat">

            <div style="text-align: center; font-size: 26px; position: absolute; top: 100px; width: 100%; text-shadow: 1px 1px 2px white, -1px -1px 2px white, -4px 4px 4px white, -4px 4px 4px white">
                {{$message}}
            </div>

        </div>
    </script>

</head>
<body style="height: 100%">
    <div class="container" style="height: 100%">
        <div ui-view style="height: 100%">
        </div>
    </div>

    <!-- 3rd party libraries -->
    <script src="/Scripts/angular.js"></script>
    <script src="/Scripts/angular-sanitize.js"></script>
    <script src="/Scripts/angular-ui/ui-bootstrap.js"></script>
    <script src="/Scripts/angular-ui/ui-bootstrap-tpls.js"></script>
    <script src="/Scripts/angular-route.min.js"></script>
    <script src="/Scripts/angular-local-storage.min.js"></script>
    <script src="/Scripts/jquery-1.10.2.js"></script>
    <script src="/Scripts/jquery.signalR-2.2.1.min.js"></script>

    <script src="~/Scripts/angular-file-upload/dist/angular-file-upload.js"></script>
    <script src="~/Scripts/angular-route.js"></script>
    <script src="~/Scripts/angular-ui-router.js"></script>
    <script src="~/Scripts/angular-animate.js"></script>
    <script src="~/Scripts/angular-validation-match.js"></script>
    <script src="~/Scripts/angular-http-loader.js"></script>
    <script src="~/Scripts/angular-busy.js"></script>
    <!-- Load app main script -->

    <script type="text/javascript" src="/signalr/hubs"></script>

    <script src="/AngularPart/app.js"></script>
   @*<script src="~/AngularPart/cgBusyDefaults.js"></script>*@
