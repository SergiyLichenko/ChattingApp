    <script src="~/AngularPart/Controllers/EditChatController.js"></script>


    <!-- Load Directives -->
    <script src="~/AngularPart/directives/ngThumbs.js"></script>
    <script src="~/AngularPart/directives/scroll.js"></script>
    <script src="~/AngularPart/directives/comparePassword.js"></script>
